    "type": "library",
    "require": {
        "omnipay/common": "^3",
        "php-http/guzzle6-adapter": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kuang Jiaye",
            "email": "kuangjy@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Omnipay\\QPay\\": "src/",
            "Omnipay\\QPay\\Tests\\": "tests/"
        }
    }
}
